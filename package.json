    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.7.1",
    "unit.js": "^2.0.0"
